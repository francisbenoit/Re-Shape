{
  "name": "Re-Context",
  "version": "1",
  "manifest_version": 2,
  "description": "Re-contextualize web pages",
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [{
    "js": ["style-script.js", "jquery-3.1.1.min.js"],
    "matches": ["http://*/*"],
    "matches": ["https://*/*"]
    }],
    "browser_action": {
      "default_title": "Re-Context",
      "default_icon": "logo16.png"
    },
    "permissions": [
      "background",
      "storage",
      "tabs",
      "*://*/*",
      "http://*/*",
      "https://*/*"
    ]
}
